<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="github-openstack"
            fetch="git://github.com/"
            review="review.openstack.org" />

    <default remote="github-openstack" revision="master" />

    <project path="nova" name="openstack/nova" />
    <project path="glance" name="openstack/glance" />
    <project path="keystone" name="openstack/keystone" />
    <project path="swift" name="openstack/swift" />
    <project path="quantum" name="openstack/quantum" />
    <project path="melange" name="openstack/melange" />
    <project path="openstack/common" name="openstack/openstack-common" />

    <project path="openstack/nose_plugin"
             name="openstack-dev/openstack-nose" />
    <project path="devstack"
             name="openstack-dev/devstack" />

    <project path="clients/melange"
             name="openstack/python-melangeclient" />
    <project path="clients/nova"
             name="openstack/python-novaclient" />
    <project path="clients/keystone"
             name="openstack/python-keystoneclient" />
</manifest>
